control.createChannelArea = function() {
alert('build channel detail area for channel');
this.container = document.createElement('div');
this.container.style.position = 'absolute';
this.container.style.left = '60%';
this.container.style.top = '0px';
this.container.style.width = '40%';
this.container.style.height = '81%';
this.container.style.background = '#35363B';
document.getElementById('').appendChild(this.container);
return true;
}
control.initChannelScreen = function(childID) {
alert('load channel detail area for channel' + childID);
return true;
};
control.myInit = function() {
var cssCode = " .button, .slider_stroke, .slider { border-radius:15px; } .widget{ opacity: 0.8} #SelectedInterfacePage { background: url(http://fc02.deviantart.net/fs18/i/2007/220/5/a/Techno_blur_wallpaper_v3_by_LyX91.jpg)} ";
var styleElement = document.createElement("style");
styleElement.type = "text/css";
styleElement.appendChild(document.createTextNode(cssCode));
document.getElementsByTagName("head")[0].appendChild(styleElement);
};
